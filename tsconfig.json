{
  "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "moduleResolution": "node",
      "ignoreDeprecations": "6.0",
      "jsx": "react-jsx",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["src/*"]
      }
    },
  "include": ["src/**/*", "src/types/cors.d.ts", "api"],  // Add the custom declaration file here
  "exclude": ["node_modules"]
}
